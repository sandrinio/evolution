    <section id="content">
        <div class="content-wrap">
            <div class="section">
                <div class="container clearfix">
                    <div class="content-wrap">

                                <table id="datatable1" class="table table-striped table-bordered" cellspacing="0" width="100%">
                                    <thead>
                                    <a href="/home/new-post" class="button button-border button-rounded button-fill button-aqua"><span>New Post</span></a>
                                    <tr>
                                        <th>Title</th>
                                        <th>Tag</th>
                                        <th>By</th>
                                        <th>Date</th>
                                        <th>Link</th>
                                    </tr>
                                    </thead>
                                    <tfoot>
                                    <tr>
                                        <th>Title</th>
                                        <th>Tag</th>
                                        <th>By</th>
                                        <th>Date</th>
                                        <th>Link</th>
                                    </tr>
                                    </tfoot>
                                    <tbody>
                                    <% postContent.forEach(function(content) { %>
                                    <tr>
                                        <td><a href="/home/show/<%= content._id %>"><%= content.title %></a></td>
                                        <td><%= content.tag %></td>
                                        <td><%= content.author.firstname + " " + content.author.lastname %></td>
                                        <td><%= content.date.toDateString() %></td>
                                        <td><a href="/home/show/<%= content._id %>">More</a><% if(currentUser.permission === 'Admin'){ %>
                                            <a class="label label-warning" href="#">Edit</a>
                                            <form class="inline-block del-form" action="/home/<%= content._id %>?_method=delete" method="POST"><button class="label label-danger" href="#">Del</button></form> <% } %> </td>
                                    </tr>
                                    <% }) %>
                                    </tbody>
                                </table>
