============================================= -->
<div id="wrapper" class="clearfix">

    <!-- Header
    ============================================= -->
    <div id="header-trigger">
        <i class="icon-line-menu"></i>
        <i class="icon-line-cross"></i>
    </div>
    <header id="header" class="no-sticky">

        <div id="header-wrap">

            <div class="container clearfix">

                <div id="primary-menu-trigger"><i class="icon-reorder"></i></div>

                <!-- Logo
                ============================================= -->
                <div id="logo">
                    <a href="/home" class="standard-logo" data-dark-logo="../../images/geocell-logo.png">
                        <img src="../../images/geocell-logo.png">
                    </a>
                    <a href="/home" class="retina-logo" data-dark-logo="../../images/geocell-logo.png">
                        <img src="../../images/geocell-logo.png" >
                    </a>
                </div>

                <!-- Primary Navigation
                ============================================= -->
                <nav id="primary-menu">

                    <ul>
                        <% if(currentUser.permission === 'Admin'){ %>
                        <li><a href="/register"><div>რეგისტრაცია</div></a></li>
                        <li><a href="/admin-panel"><div>ადმინ</div></a></li>
                        <% } %>
                        <li><a href="/home"><div>მთავარი</div></a></li>
                        <li><a href="/evolution"><div>შეფასება</div></a></li>
                        <li><a href="#"><div><em><%= currentUser.fullname %></em></div></a></li>
                    </ul>



                </nav><!-- #primary-menu end -->

            </div>

        </div>

    </header><!-- #header end -->


    <!-- Content
    ============================================= -->
    <section id="content">

        <div class="content-wrap">

            <div class="container clearfix">


            </div>

        </div>

    </section><!-- #content end -->

    <!-- Footer
    ============================================= -->
</div>
    <footer id="footer" class="dark">



