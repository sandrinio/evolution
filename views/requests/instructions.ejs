<!-- Content
============================================= -->
<section id="content">
    <div class="content-wrap">

            <div class="container clearfix">
                <div class="content-wrap">
                    <table id="datatable1" class="table table-striped table-bordered" cellspacing="0" width="100%">
                        <thead>
                        <tr>
                            <th>Title</th>
                            <th>Tag</th>
                            <th>By</th>
                            <th>Date</th>
                            <th>Link</th>
                        </tr>
                        </thead>
                        <tfoot>
                        <tr>
                            <th>Title</th>
                            <th>Tag</th>
                            <th>By</th>
                            <th>Date</th>
                            <th>Link</th>
                        </tr>
                        </tfoot>
                        <tbody>
                        <% reqsContent.forEach(function(content) { %>
                        <tr>
                            <td><a href="/home/show/<%= content._id %>"><%= content.title %></a><span class="pull-right"><i class="icon-comments"></i> <%= content.comments.length %></span></td>
                            <td><%= content.tag %> </td>
                            <td><%= content.author.firstname + " " + content.author.lastname %></td>
                            <td><%= content.date.toString().slice(0, 21) %></td>
                            <td><a href="/home/show/<%= content._id %>">More</a><% if(currentUser.permission === 'Admin'){ %>
                                <a class="label label-warning" href="/post/<%= content._id %>/edit">Edit</a>
                                <form class="inline-block del-form" action="/home/<%= content._id %>?_method=delete" method="POST"><button class="label label-danger" href="#">Del</button></form> <% } %> </td>
                        </tr>
                        <% }) %>
                        </tbody>
                    </table>
                </div>


            </div>
        <div id="container-floating">
            <div id="floating-button" data-toggle="tooltip" data-placement="left" data-original-title="New Post">
                <p class="plus">+</p>
                <a href="/home/new-post"><img class="edit" src="https://ssl.gstatic.com/bt/C3341AA7A1A076756462EE2E5CD71C11/1x/bt_compose2_1x.png"></a>
            </div>
